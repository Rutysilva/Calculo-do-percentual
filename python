sp = float(67.83643)
rj = float(36.67866)
mg = float(29.22988)
es = float(27.16548)
out = float(19.84953)
soma = float(sp + rj + mg + es + out)
print(soma,'\n')
persp = ((sp/soma)*100)
perrj = ((rj/soma)*100)
permg = ((mg/soma)*100)
peres = ((es/soma)*100)
perout = ((out/soma)*100)

print('***percentual de representação***','\n')
print('SP = {}'.format(persp))
print('RJ = {}'.format(perrj))
print('MG = {}'.format(permg))
print('ES = {}'.format(peres))
print('Outros = {}'.format(perout))
